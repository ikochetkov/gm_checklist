        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	if(current.subject.indexOf('New GM Identified')>-1){
		var dmsrevGr = new GlideRecord('dms_document_revision');
		dmsrevGr.addQuery('stage', 'published');
		dmsrevGr.addQuery('document.name', 'GM Changeover Checklist');
		dmsrevGr.query();
		if(dmsrevGr.next()){
			new GlideSysAttachment.copy('dms_document_revision ', dmsrevGr.getUniqueValue(), current.getTableName(), current.sys_id);
		}
	}

})(current, previous);]]></script>
