        <name>callGetAllDepartmentsInHierarchyWithMyDep</name>
        <script><![CDATA[function callGetAllDepartmentsInHierarchyWithMyDep(){

	function callGetAllDepartmentsInHierarchy() {
		return new global.HierarchyUtils().getMyChildDepartmentsHierarchically();
	}

	var childDepsInfo = callGetAllDepartmentsInHierarchy();
	var childNames = [];
	childDepsInfo.forEach(function(child){
		childNames.push(child.name);
	});

	return childNames;
}
